{
  "name": "vscode",
  "cmd": "code",
  "type": "file",
  "format": "json",
  "configFilePath": {
    "win32": "%APPDATA%\\Code\\User\\settings.json",
    "darwin": "$HOME/Library/Application Support/Code/User/settings.json",
    "linux": "$HOME/.config/Code/User/settings.json"
  },
  "httpProxyFields": ["http.proxy"],
  "extraFields": {
    "http.proxyStrictSSL": false
  }
}
